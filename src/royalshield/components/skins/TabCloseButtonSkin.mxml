<?xml version="1.0" encoding="utf-8"?>

<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009"
             xmlns:s="library://ns.adobe.com/flex/spark"
             minHeight="12"
             minWidth="12"
             alpha.disabled="0.5">
    
    <!-- host components -->
    <fx:Metadata>
        [HostComponent("spark.components.Button")]
    </fx:Metadata>
    
    <!-- declations -->
    <fx:Declarations>
        <s:SolidColorStroke id="stroke1"
                            weight="2"
                            color="0x888888"
                            color.over="0xDFDFDF"/>
    </fx:Declarations>
    
    <!-- states -->
    <s:states>
        <s:State name="up" />
        <s:State name="over" />
        <s:State name="down" />
        <s:State name="disabled" />
    </s:states>
    
    <s:Rect width="10" height="10">
        <s:fill>
            <s:SolidColor color="0x000000" alpha="0"/>
        </s:fill>
    </s:Rect>
    
    <s:Line xFrom="2"
            yFrom="2"
            xTo="8"
            yTo="8"
            stroke="{stroke1}" />
    
    <s:Line xFrom="8"
            yFrom="2"
            xTo="2"
            yTo="8"
            stroke="{stroke1}" />
</s:SparkSkin>
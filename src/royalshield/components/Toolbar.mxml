         xmlns:s="library://ns.adobe.com/flex/spark"
         xmlns:mx="library://ns.adobe.com/flex/mx">
    
    <!-- highlight -->
    <s:Rect left="-1" right="-1" top="1" bottom="0">
        <s:stroke>
            <s:SolidColorStroke color="0x636363"/>
